@{
    ViewBag.Title = "Home Page";
}
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
<script src="~/Content/querycount/js/jquery.countdown.js" type="text/javascript"></script>
<link href="~/Content/querycount/css/media.css" rel="stylesheet" type="text/css" />
<script type="text/javascript">
      $(function(){
        $(".digits").countdown({
            image: "~/Content/querycount/img/digits.png",
          format: "dd:hh:mm:ss",
          endTime: new Date(2018, 04, 1)
        });
      });
</script>
